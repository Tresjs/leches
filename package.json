    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/ui": "^0.34.1",
    "@vueuse/core": "^10.3.0",
    "@vueuse/shared": "^10.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "eslint-plugin-vue": "^9.17.0",
    "histoire": "^0.17.0",
    "kolorist": "^1.8.0",
    "pathe": "^1.1.1",
    "prettier": "^3.0.1",
    "release-it": "^16.1.4",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.2",
